<!-- WithSecure Decoders -->
<decoder name="withsecure-collector">
    <program_name>withsecure-collector</program_name>
</decoder>

<decoder name="withsecure-fields">
    <parent>withsecure-collector</parent>
    <regex>action="(\.+)"</regex>
    <order>ws.action</order>
</decoder>

